{
    "version": 2,
    "routes": [
      {
        "src": "/_next(.*)",
        "dest": "/_next$1"
      },
      {
        "src": "/api/(.*)",
        "dest": "/dist/src/api/$1"
      },
      {
        "src": "/uploads/(.*)",
        "dest": "/dist/public/uploads/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/dist/src/index.js"
      }
    ]
  }
  